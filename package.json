{
  "name": "massive-wax",
  "description": "It does repetitive jobs on GitHub repos for you!",
  "version": "1.1.0",
  "author": "Manuel Spigolon @Eomm",
  "bin": {
    "massive-wax": "./bin/run"
  },
  "bugs": "https://github.com/Eomm/massive-wax/issues",
  "dependencies": {
    "@oclif/core": "^1.0.0",
    "@oclif/plugin-help": "^5.0.0",
    "@oclif/plugin-not-found": "^2.3.1",
    "@octokit/rest": "^18.12.0",
    "p-map": "^4.0.0",
    "p-retry": "^4.6.1",
    "parse-github-url": "^1.0.2",
    "simple-git": "^3.2.6"
  },
  "devDependencies": {
    "@oclif/test": "^2.0.0",
    "oclif": "^2.6.1",
    "standard": "^16.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/Eomm/massive-wax",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "bin": "massive-wax",
    "dirname": "massive-wax",
    "commands": "./src/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found"
    ],
    "topicSeparator": " "
  },
  "repository": "Eomm/massive-wax",
  "scripts": {
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "postpack": "rimraf oclif.manifest.json",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "npm run lint && tap --no-check-coverage test/**/*.test.js",
    "prepublish": "npm run readme",
    "readme": "oclif readme && git add README.md"
  }
}
